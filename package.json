{
  "name": "naive-definitions",
  "displayName": "Naive Definitions",
  "description": "A naive, find-in-files implementation of going to the definition of a symbol",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:naive-definitions.goToDefinition"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "naive-definitions.goToDefinition",
        "title": "[Naive] Go To Definition"
      }
    ],
    "menus": {
      "editor/context": [
        {
            "command": "naive-definitions.goToDefinition",
            "group": "naive-definitions",
            "when": "editorHasSelection && editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
